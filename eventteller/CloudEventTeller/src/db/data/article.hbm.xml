<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="db.data">
	<class
		name="article"
		table="article" dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		</id>
		<property
			name="title"
			column="title"
			type="string"
			not-null="true"
			length="1000"
		/>
		<property
			name="publishtime"
			column="publishtime"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="mainparagraph"
			column="mainparagraph"
			type="string"
			not-null="false"
		/>
		<property
			name="extracttime"
			column="extracttime"
			type="java.util.Date"
			not-null="true"
			length="14"
		/>
		<property
			name="crawltime"
			column="crawltime"
			type="java.util.Date"
			not-null="true"
			length="14"
		/>
		<property
			name="mainwords"
			column="mainwords"
			type="string"
			not-null="false"
		/>
		<property
			name="titlewords"
			column="titlewords"
			type="string"
			not-null="false"
			length="1000"
		/>
		<property
			name="summary"
			column="summary"
			type="string"
			not-null="false"
		/>
		<property
			name="eventid"
			column="eventid"
			type="integer"
			not-null="false"
			length="32"
		/>
		<property
			name="taskstatus"
			column="taskstatus"
			type="integer"
			not-null="false"
			length="32"
		/>
		<property
			name="img"
			column="img"
			type="string"
			not-null="false"
			length="1000"
		/>
		<property
			name="subtopicid"
			column="subtopicid"
			type="integer"
			not-null="false"
			length="32"
		/>
	</class>	
</hibernate-mapping>